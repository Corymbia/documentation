<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="delete_user">
	<title>Delete a User</title>
	<shortdesc>To delete a user perform the steps listed in this topic.</shortdesc>
	<taskbody>
		<context></context>
	
			<steps>
				<step>
					<cmd>Enter the following command, replacing the variable
						where noted:</cmd>
					<info>
						<codeblock>euare-user-del -u &lt;user_name> </codeblock>
					</info>
				</step>
			</steps>
		</taskbody>
	</task>
