<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="add_user">
	<title>Add a User</title>
	<shortdesc>To add a user perform the steps in this topic.</shortdesc>
	<taskbody>
		<context></context>
			<steps>
				<step>
					<cmd>Enter the following command, replacing the variable
						where noted:</cmd>
					<info>
						<codeblock>euare-user-create -u &lt;user_name> </codeblock>
					</info>
				</step>
			</steps>
	</taskbody>
</task>
