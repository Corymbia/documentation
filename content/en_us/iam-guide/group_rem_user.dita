<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="group_rem_user">
	<title>Remove a User from a Group</title>
	<shortdesc>To remove a user from a group perform the steps listed in this topic.</shortdesc>
	<prolog>
		<metadata>
			<keywords>
				<indexterm> access tasks <indexterm>groups <indexterm>remove
							user</indexterm>
					</indexterm>
				</indexterm>
			</keywords>
		</metadata>
	</prolog>
	
		<taskbody>
			<context></context>
			<steps>
				<step>
					<cmd>Enter the following command:</cmd>
					<info>
						<codeblock>euare-groupremoveuser -g &lt;group_name> -u &lt;user-name> </codeblock>
					</info>
				</step>
			</steps>

		</taskbody>
</task>
