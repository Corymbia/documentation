<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="account_rename">
	<title>Rename an Account</title>
	<shortdesc>To rename an account perform the steps listed in this topic.</shortdesc>
	<prolog>
		<metadata>
			<keywords>
				<indexterm> access tasks <indexterm>accounts <indexterm>rename
							an account</indexterm>
					</indexterm>
				</indexterm>
			</keywords>
		</metadata>
	</prolog>
	<taskbody>
		<context>
			
				<p>To change an account's name:</p>
			</context>
			<steps>
				<step>
					<cmd>Enter the following command:</cmd>
					<info>
						<codeblock>uare-accountaliascreate -a &lt;new_name></codeblock>
					</info>
					<!--<stepresult>Eucalyptus does not return a message.</stepresult>-->
				</step>
			</steps>
		</taskbody>
	
</task>
