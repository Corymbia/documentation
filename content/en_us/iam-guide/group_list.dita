<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="group_list">
	<title>List Groups</title>
	<shortdesc>To list groups perform the steps listed in this topic.</shortdesc>
	<prolog>
		<metadata>
			<keywords>
				<indexterm> access tasks <indexterm>groups <indexterm>list
							all</indexterm>
					</indexterm>
				</indexterm>
			</keywords>
		</metadata>
	</prolog>
	
		<taskbody>
			<context></context>
			<steps>
				<step>
					<cmd>Enter the following command:</cmd>
					<info>
						<codeblock>euare-grouplistbypath</codeblock>
					</info>
					<stepresult>Eucalyptus returns a list of paths followed by
						the ARNs for the groups in each path. For example:
						<codeblock>arn:aws:iam::eucalyptus:group/groupa</codeblock>
					</stepresult>
				</step>
			</steps>
		</taskbody>
	
</task>
